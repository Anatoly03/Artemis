adminUser=${bamboo.artemis_admin_username}
adminPassword=${bamboo.artemis_admin_password}

# Cypress settings
CYPRESS_baseUrl="https://artemis-app"
CYPRESS_adminUsername=$adminUser
CYPRESS_adminPassword=$adminPassword
CYPRESS_username=${bamboo.cypress_username_template}
CYPRESS_password=${bamboo.cypress_password_template}
CYPRESS_allowGroupCustomization=true
CYPRESS_studentGroupName=artemis-e2etest-students
CYPRESS_tutorGroupName=artemis-e2etest-tutors
CYPRESS_editorGroupName=artemis-e2etest-editors
CYPRESS_instructorGroupName=artemis-e2etest-instructors

# Artemis settings
DATASOURCE_URL="jdbc:mysql://artemis-mysql:3306/Artemis?createDatabaseIfNotExist=true&allowPublicKeyRetrieval=true&useUnicode=true&characterEncoding=utf8&useSSL=false&useLegacyDatetimeCode=false&serverTimezone=UTC"
UM_URL="https://jira-prelive.ase.in.tum.de"
CI_URL="https://bamboo-prelive.ase.in.tum.de"
SCM_URL="https://bitbucket-prelive.ase.in.tum.de"
SCM_USER=$bamboo_jira_prelive_admin_user
SCM_PASSWORD=$bamboo_jira_prelive_admin_password
UM_USER=$bamboo_jira_prelive_admin_user
UM_PASSWORD=$bamboo_jira_prelive_admin_password
CI_USER=$bamboo_jira_prelive_admin_user
CI_PASSWORD=$bamboo_jira_prelive_admin_password
CI_AUTH_TOKEN=${bamboo.ARTEMIS_CONTINUOUS_INTEGRATION_ARTEMIS_AUTHENTICATION_TOKEN_VALUE_SECRET}
CI_TOKEN=${bamboo.ARTEMIS_CONTINUOUS_INTEGRATION_TOKEN_SECRET}
# TODO: don't know why the hostname would be needed here as cypress just runs against the other docker container
# TODO: and it doesn't seem to be reachable from the outside (tested from eduVPN)
SERVER_URL="http://$(hostname):$artemisPort"
SERVER_PORT=$artemisPort
ADMIN_USERNAME=$adminUser
ADMIN_PASSWORD=$adminPassword
